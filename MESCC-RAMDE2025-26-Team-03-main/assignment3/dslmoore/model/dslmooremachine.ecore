      <details key="eventNameMustStartWithUppercase" value="self.name.trim().substring(1, 1).toUpper().compareTo(self.name.trim().substring(1, 1)) = 0"/>
      <details key="nameMustHaveThirtyCharactersAtMax" value="self.name.trim().size() &lt;= 30"/>
      <details key="nameShouldNotContainSpecialCharacters" value="self.name.characters()->forAll(c | '\\|!&quot;@#£$§%&amp;/{[]=}?\'»«+*¨`´~^.:,;'.indexOf(c) = 0)"/>
      <details key="nameShouldNotContainNumbers" value="self.name.characters()->forAll(c | '0123456789'.indexOf(c) = 0)"/>
    </eAnnotations>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="name" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="transition" lowerBound="1"
        upperBound="-1" eType="#//Transition" eOpposite="#//Transition/event"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Transition">
    <eAnnotations source="http://www.eclipse.org/emf/2002/Ecore">
      <details key="constraints" value="transitionNameMustHaveAtLeastThreeCharactersExcludingRepeatingEmptySpaces transitionNameMustStartWithUppercase nameMustHaveThirtyCharactersAtMax nameShouldNotContainSpecialCharacters nameShouldNotContainNumbers"/>
    </eAnnotations>
    <eAnnotations source="http://www.eclipse.org/emf/2002/Ecore/OCL/Pivot">
      <details key="transitionNameMustHaveAtLeastThreeCharactersExcludingRepeatingEmptySpaces"
          value="self.name.trim().size() >= 3"/>
      <details key="transitionNameMustStartWithUppercase" value="self.name.trim().substring(1, 1).toUpper().compareTo(self.name.trim().substring(1, 1)) = 0"/>
      <details key="nameMustHaveThirtyCharactersAtMax" value="self.name.trim().size() &lt;= 30"/>
      <details key="nameShouldNotContainSpecialCharacters" value="self.name.characters()->forAll(c | '\\|!&quot;@#£$§%&amp;/{[]=}?\'»«+*¨`´~^.:,;'.indexOf(c) = 0)"/>
      <details key="nameShouldNotContainNumbers" value="self.name.characters()->forAll(c | '0123456789'.indexOf(c) = 0)"/>
    </eAnnotations>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="name" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="event" lowerBound="1" eType="#//Event"
        eOpposite="#//Event/transition"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="targetState" lowerBound="1"
        eType="#//State" eOpposite="#//State/incoming"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="sourceState" lowerBound="1"
        eType="#//State" eOpposite="#//State/outgoing"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Output">
    <eAnnotations source="http://www.eclipse.org/emf/2002/Ecore">
      <details key="constraints" value="outputNameMustHaveAtLeastThreeCharactersExcludingRepeatingEmptySpaces outputNameMustStartWithUppercase nameMustHaveThirtyCharactersAtMax nameShouldNotContainSpecialCharacters nameShouldNotContainNumbers"/>
    </eAnnotations>
    <eAnnotations source="http://www.eclipse.org/emf/2002/Ecore/OCL/Pivot">
      <details key="outputNameMustHaveAtLeastThreeCharactersExcludingRepeatingEmptySpaces"
          value="self.name.trim().size() >= 3"/>
      <details key="outputNameMustStartWithUppercase" value="self.name.trim().substring(1, 1).toUpper().compareTo(self.name.trim().substring(1, 1)) = 0"/>
      <details key="nameMustHaveThirtyCharactersAtMax" value="self.name.trim().size() &lt;= 30"/>
      <details key="nameShouldNotContainSpecialCharacters" value="self.name.characters()->forAll(c | '\\|!&quot;@#£$§%&amp;/{[]=}?\'»«+*¨`´~^.:,;'.indexOf(c) = 0)"/>
      <details key="nameShouldNotContainNumbers" value="self.name.characters()->forAll(c | '0123456789'.indexOf(c) = 0)"/>
    </eAnnotations>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="name" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="state" lowerBound="1" upperBound="-1"
        eType="#//State" eOpposite="#//State/output"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="InitialNode">
    <eStructuralFeatures xsi:type="ecore:EReference" name="initialState" lowerBound="1"
        eType="#//State" eOpposite="#//State/initialnode"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="State">
    <eAnnotations source="http://www.eclipse.org/emf/2002/Ecore">
      <details key="constraints" value="stateNameMustHaveAtLeastThreeCharactersExcludingRepeatingEmptySpaces stateNameMustStartWithUppercase stateShouldBeTheTargetOfAtLeastOneTransition stateShouldBeTheSourceOfAtLeastOneTransition nameMustHaveThirtyCharactersAtMax nameShouldNotContainSpecialCharacters nameShouldNotContainNumbers"/>
    </eAnnotations>
    <eAnnotations source="http://www.eclipse.org/emf/2002/Ecore/OCL/Pivot">
      <details key="stateNameMustHaveAtLeastThreeCharactersExcludingRepeatingEmptySpaces"
          value="self.name.trim().size() >= 3"/>
      <details key="stateNameMustStartWithUppercase" value="self.name.trim().substring(1, 1).toUpper().compareTo(self.name.trim().substring(1, 1)) = 0"/>
      <details key="stateShouldBeTheTargetOfAtLeastOneTransition" value="self.incoming -> size() >= 1"/>
      <details key="stateShouldBeTheSourceOfAtLeastOneTransition" value="self.outgoing -> size() >= 1"/>
      <details key="nameMustHaveThirtyCharactersAtMax" value="self.name.trim().size() &lt;= 30"/>
      <details key="nameShouldNotContainSpecialCharacters" value="self.name.characters()->forAll(c | '\\|!&quot;@#£$§%&amp;/{[]=}?\'»«+*¨`´~^.:,;'.indexOf(c) = 0)"/>
      <details key="nameShouldNotContainNumbers" value="self.name.characters()->forAll(c | '0123456789'.indexOf(c) = 0)"/>
    </eAnnotations>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="name" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="output" lowerBound="1"
        eType="#//Output" eOpposite="#//Output/state"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="incoming" lowerBound="1"
        upperBound="-1" eType="#//Transition" eOpposite="#//Transition/targetState"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="outgoing" lowerBound="1"
        upperBound="-1" eType="#//Transition" eOpposite="#//Transition/sourceState"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="initialnode" eType="#//InitialNode"
        eOpposite="#//InitialNode/initialState"/>
  </eClassifiers>
</ecore:EPackage>
