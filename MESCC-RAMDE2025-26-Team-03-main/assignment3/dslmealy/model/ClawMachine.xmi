<?xml version="1.0" encoding="UTF-8"?>
<dslmealymachine:MealyMachine
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:dslmealymachine="http://www.example.org/dslmealymachine"
    name="Claw Machine">
  <event name="Press Action button"
      transition="//@transition.9 //@transition.10 //@transition.11 //@transition.12"/>
  <event name="Move lever - Right"
      transition="//@transition.1 //@transition.5"/>
  <event name="Move lever - Left"
      transition="//@transition.2 //@transition.6"/>
  <event name="Move lever - Up"
      transition="//@transition.3 //@transition.7"/>
  <event name="Move lever - Down"
      transition="//@transition.4 //@transition.8"/>
  <event name="Press Power button"
      transition="//@transition.0 //@transition.13 //@transition.14"/>
  <transition
      name="Machine On"
      event="//@event.5"
      output="//@output.1"
      targetState="//@state.0"
      sourceState="//@state.3"/>
  <transition
      name="Move claw right - Grabber"
      event="//@event.1"
      output="//@output.2"
      targetState="//@state.0"
      sourceState="//@state.0"/>
  <transition
      name="Move claw left - Grabber"
      event="//@event.2"
      output="//@output.3"
      targetState="//@state.0"
      sourceState="//@state.0"/>
  <transition
      name="Move claw forwards - Grabber"
      event="//@event.3"
      output="//@output.4"
      targetState="//@state.0"
      sourceState="//@state.0"/>
  <transition
      name="Move claw backwards -  Grabber"
      event="//@event.4"
      output="//@output.5"
      targetState="//@state.0"
      sourceState="//@state.0"/>
  <transition
      name="Move claw right - Dropper"
      event="//@event.1"
      output="//@output.2"
      targetState="//@state.1"
      sourceState="//@state.1"/>
  <transition
      name="Move claw left  - Dropper"
      event="//@event.2"
      output="//@output.3"
      targetState="//@state.1"
      sourceState="//@state.1"/>
  <transition
      name="Move claw forwards - Dropper"
      event="//@event.3"
      output="//@output.4"
      targetState="//@state.1"
      sourceState="//@state.1"/>
  <transition
      name="Move claw backwards - Dropper"
      event="//@event.4"
      output="//@output.5"
      targetState="//@state.1"
      sourceState="//@state.1"/>
  <transition
      name="Descend"
      event="//@event.0"
      output="//@output.6"
      targetState="//@state.2"
      sourceState="//@state.0"/>
  <transition
      name="Pinch"
      event="//@event.0"
      output="//@output.7"
      targetState="//@state.4"
      sourceState="//@state.2"/>
  <transition
      name="Ascend"
      event="//@event.0"
      output="//@output.8"
      targetState="//@state.1"
      sourceState="//@state.4"/>
  <transition
      name="Release"
      event="//@event.0"
      output="//@output.9"
      targetState="//@state.0"
      sourceState="//@state.1"/>
  <transition
      name="Grabber to Off"
      event="//@event.5"
      output="//@output.0"
      targetState="//@state.3"
      sourceState="//@state.0"/>
  <transition
      name="Dropper to Off"
      event="//@event.5"
      output="//@output.0"
      targetState="//@state.3"
      sourceState="//@state.1"/>
  <initialnode
      initialState="//@state.3"/>
  <state name="On -  Grabber mode"
      incoming="//@transition.0 //@transition.1 //@transition.2 //@transition.3 //@transition.4 //@transition.12"
      outgoing="//@transition.1 //@transition.2 //@transition.3 //@transition.4 //@transition.9 //@transition.13"/>
  <state name="On - Dropper mode"
      incoming="//@transition.5 //@transition.6 //@transition.7 //@transition.8 //@transition.11"
      outgoing="//@transition.5 //@transition.6 //@transition.7 //@transition.8 //@transition.12 //@transition.14"/>
  <state name="Claw down"
      incoming="//@transition.9"
      outgoing="//@transition.10"/>
  <state name="Off"
      incoming="//@transition.13 //@transition.14"
      outgoing="//@transition.0"
      initialnode="//@initialnode"/>
  <state name="Pinched"
      incoming="//@transition.10"
      outgoing="//@transition.11"/>
  <output name="Machine Off"
      transition="//@transition.13 //@transition.14"/>
  <output name="Machine On"
      transition="//@transition.0"/>
  <output name="Claw moves right"
      transition="//@transition.5 //@transition.1"/>
  <output name="Claw moves left"
      transition="//@transition.2 //@transition.6"/>
  <output name="Claw moves forwards"
      transition="//@transition.7 //@transition.3"/>
  <output name="Claw moves backwards"
      transition="//@transition.8 //@transition.4"/>
  <output name="Claw moves downwards"
      transition="//@transition.9"/>
  <output name="Claw pinches"
      transition="//@transition.10"/>
  <output name="Claw moves upwards"
      transition="//@transition.11"/>
  <output name="Claw releases"
      transition="//@transition.12"/>
</dslmealymachine:MealyMachine>
